{
  "name": "playbookd",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" NEXT_DISABLE_SWC=true UV_THREADPOOL_SIZE=1 FORCE_COLOR=0 NO_WORKER=1 JEST_WORKER_ID=1 next dev",
    "dev:3002": "cross-env PORT=3002 NODE_OPTIONS=\"--max-old-space-size=8192\" NEXT_DISABLE_SWC=true UV_THREADPOOL_SIZE=1 FORCE_COLOR=0 NO_WORKER=1 JEST_WORKER_ID=1 next dev",
    "build": "next build",
    "build:deploy": "cross-env NODE_ENV=production next build",
    "export": "next build && next export",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:security": "vitest run tests/security",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:security && npm run test:e2e",
    "emulator:start": "firebase emulators:start --only firestore",
    "emulator:test": "firebase emulators:exec --only firestore \"npm run test:security\"",
    "functions:serve": "firebase emulators:start --only functions,firestore,hosting",
    "functions:deploy": "firebase deploy --only functions",
    "seed": "tsx seed/seed.ts",
    "seed:comprehensive": "tsx seed/comprehensive-sample-data.ts",
    "seed:advanced": "tsx seed/advanced-sample-data.ts",
    "seed:all": "tsx seed/master-seed.ts",
    "deploy:indexes": "node scripts/deploy-indexes.js",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "deploy:firestore": "firebase deploy --only firestore",
    "setup:firestore": "npm run deploy:rules && npm run deploy:indexes",
    "role:set": "tsx scripts/set-role.ts"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.17.1",
    "@google-cloud/video-transcoder": "^4.4.0",
    "@google/genai": "^1.17.0",
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@stripe/stripe-js": "^2.4.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/qrcode": "^1.5.5",
    "@uppy/core": "^5.1.1",
    "@uppy/dashboard": "^5.0.3",
    "@uppy/react": "^5.1.0",
    "@uppy/tus": "^5.0.2",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.3",
    "firebase": "^12.2.1",
    "firebase-admin": "^13.5.0",
    "google-auth-library": "^9.15.1",
    "ioredis": "^5.8.0",
    "isomorphic-dompurify": "^2.28.0",
    "lucide-react": "^0.542.0",
    "next": "^14.2.15",
    "openai": "^5.19.1",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.4",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^10.1.0",
    "react-player": "^2.16.0",
    "remark-gfm": "^4.0.1",
    "resend": "^6.1.0",
    "sonner": "^2.0.7",
    "stripe": "^14.0.0",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.23.8",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@firebase/rules-unit-testing": "^5.0.0",
    "@hookform/resolvers": "^3.6.0",
    "@playwright/test": "^1.56.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.30",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.17",
    "critters": "^0.0.23",
    "cross-env": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.15",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.1.1",
    "jsdom": "^27.0.0",
    "null-loader": "^4.0.1",
    "postcss": "^8.4.35",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.6",
    "ts-jest": "^29.4.4",
    "tsx": "^4.7.0",
    "typescript": "^5.4.5",
    "vitest": "^3.2.4"
  }
}
